{
  "name": "isomorphic-undici",
  "version": "1.0.5",
  "description": "isomorphic undici fetch",
  "browser": "dist/fetch-browser.js",
  "main": "dist/fetch-node.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "microbundle -i src/fetch-node.js -o dist/fetch-node.js --no-sourcemap -f cjs --generateTypes false && microbundle -i src/fetch-browser.js -o dist/fetch-browser.js --no-sourcemap -f cjs --generateTypes false",
    "release": "npm run build -s && npm publish"
  },
  "author": "free3",
  "license": "MIT",
  "homepage": "https://github.com/free3xm/isomorphic-undici#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/free3xm/isomorphic-undici.git"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "microbundle": "^0.13.3"
  }
}
